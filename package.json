{
  "name": "@mfrank4101@ptlinked-plugin",
  "version": "1.0.0",
  "description": "PTLINKED is a digital health / physical therapy company focused on creating engaging digital content and solutions that improve the musculoskeletal health, fitness, and wellness of populations globally, while also equipping providers with innovative tools that enable them to deliver convenient, accessible, and affordable care.",
  "main": "dist/jquery-ptlinked_plugin.js",
  "style": "dist/ptlinked_plugin.css",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "npm run build:css && npm run build:js",
    "build:css": "postcss src/ptlinked_plugin.css -o dist/ptlinked_plugin.css --no-map",
    "build:js": "rollup -c",
    "clear": "del-cli dist",
    "compress": "npm run compress:css && npm run compress:js",
    "compress:css": "postcss dist/ptlinked_plugin.css -u cssnano -o dist/ptlinked_plugin.min.css --no-map",
    "compress:js": "uglifyjs dist/jquery-ptlinked_plugin.js -o dist/jquery-ptlinked_plugin.min.js -c -m --comments /^!/",
    "release": "npm run clear && npm run build && npm run compress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mfrank4101/ptlinked-plugin.git"
  },
  "keywords": [
    "exercise",
    "ptlinked",
    "mdvip"
  ],
  "author": {
    "name": "Mike Frank - PTLINKED, LLC",
    "url": "https://www.ptlinked.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mfrank4101/ptlinked-plugin/issues"
  },
  "homepage": "https://github.com/mfrank4101/ptlinked-plugin#readme",
  "devDependencies": {
    "cssnano": "^4.1.10",
    "del-cli": "^3.0.1",
    "postcss": "^8.1.10",
    "postcss-cli": "^8.3.0",
    "postcss-header": "^3.0.1",
    "postcss-import": "^13.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.1",
    "puppeteer": "^5.5.0",
    "rollup": "^2.33.3",
    "uglify-js": "^3.12.0"
  }
}
